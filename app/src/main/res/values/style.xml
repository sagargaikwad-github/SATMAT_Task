        <item name="cornerSize">50%</item>

    </style>

    <style name="CustomDatePickerDialogTheme" parent="ThemeOverlay.MaterialComponents.Dialog">
        <item name="colorAccent">@color/colorPrimary</item> <!--Header Color-->
        <item name="android:windowBackground">@color/white</item> <!--Header dates bg Color-->
